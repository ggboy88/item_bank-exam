<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ggboy.exam.dao.itemBank.UserDao">

    <select id="selectUserNameById" parameterType="Integer" resultType="java.lang.String">
        SELECT
        Teacher_name
        FROM
        users
        WHERE
        id = #{id}
    </select>

</mapper>